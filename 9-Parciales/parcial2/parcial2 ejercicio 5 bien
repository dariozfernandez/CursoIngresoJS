//Debemos lograr mostrar un mensaje al presionar el botón  'MOSTRAR'.
function Mostrar()
{
	//Realizar el algoritmo que al presionar el botón "Mostrar" pida un mes del año por prompt, si es un "enero" o
	// "febrero" informar por alert “Veranito!!!!” de lo contrario informar por alert
	// “extraño enero y febrero!!!”
	

	var mes;


	mes= prompt(" ingrese un mes del año ");
	mes= mes.toLowerCase(); //toma todo en minuscula mes= mes.toUpperCase(); toma todo en mayuscula.
	

	switch (mes)

	{
		case "enero":
		case "eebrero":

			 alert(" veranito ");

			 break;


		default:

				alert( " extraño enero y eebrero ");

				
	}


	
}

